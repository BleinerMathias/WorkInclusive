<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="640.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifsp.application.controller.company.AcceptInterviewsPageUIController">
    <Button fx:id="btnhome" layoutX="7.0" layoutY="8.0" mnemonicParsing="false" onAction="#initialPage" style="-fx-border-color: default; -fx-background-color: default;" text="Inicio" />
    <Button fx:id="btnProfile" layoutX="60.0" layoutY="8.0" mnemonicParsing="false" onAction="#Account" style="-fx-border-color: default; -fx-background-color: default;" text="Perfil" />
    <Button fx:id="btnVacany" layoutX="118.0" layoutY="8.0" mnemonicParsing="false" onAction="#vacancy" style="-fx-border-color: default; -fx-background-color: default;" text="Vagas" />
    <Button fx:id="btnInterviews" layoutX="176.0" layoutY="8.0" mnemonicParsing="false" onAction="#interviews" style="-fx-border-color: default; -fx-background-color: default;" text="Entrevistas" />
    <Button fx:id="btnConfig" layoutX="253.0" layoutY="8.0" mnemonicParsing="false" onAction="#config" style="-fx-border-color: default; -fx-background-color: default;" text="Configurações" />
    <Label layoutX="83.0" layoutY="44.0" text="Combinações de entrevistas">
      <font>
         <Font size="15.0" />
      </font></Label>
    <TableView fx:id="tableView" layoutY="84.0" prefHeight="426.0" prefWidth="360.0">
        <columns>
            <TableColumn fx:id="cId" prefWidth="41.0" text="Id" />
            <TableColumn fx:id="cDateTime" minWidth="0.0" prefWidth="115.0" text="Data e Horário" />
            <TableColumn fx:id="cCandidate" minWidth="0.0" prefWidth="117.0" text="Candidato" />
         <TableColumn fx:id="cCombined" prefWidth="86.0" text="Combinação" />
        </columns>
    </TableView>
   <Button fx:id="btnDecline" layoutX="39.0" layoutY="563.0" mnemonicParsing="false" onAction="#unacceptInterview" prefHeight="40.0" prefWidth="90.0" text="Recusar">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
   <Button fx:id="btnAccept" layoutX="232.0" layoutY="563.0" mnemonicParsing="false" onAction="#acceptInterview" prefHeight="40.0" prefWidth="90.0" text="Aceitar">
      <font>
         <Font size="14.0" />
      </font>
   </Button>

</AnchorPane>
