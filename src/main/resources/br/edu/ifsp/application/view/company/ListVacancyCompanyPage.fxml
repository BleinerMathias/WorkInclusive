<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="640.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifsp.application.controller.company.ListVacancyCompanyUIController">
    <Button fx:id="btnhome" layoutX="7.0" layoutY="8.0" mnemonicParsing="false" onAction="#initialPage" style="-fx-border-color: default; -fx-background-color: default;" text="Inicio" />
    <Button fx:id="btnProfile" layoutX="60.0" layoutY="8.0" mnemonicParsing="false" onAction="#Account" style="-fx-border-color: default; -fx-background-color: default;" text="Perfil" />
    <Button fx:id="btnVacany" layoutX="118.0" layoutY="8.0" mnemonicParsing="false" onAction="#vacancy" style="-fx-border-color: default; -fx-background-color: default;" text="Vagas" />
    <Button fx:id="btnInterviews" layoutX="176.0" layoutY="8.0" mnemonicParsing="false" onAction="#interviews" style="-fx-border-color: default; -fx-background-color: default;" text="Entrevistas" />
    <Button fx:id="btnConfig" layoutX="253.0" layoutY="8.0" mnemonicParsing="false" onAction="#config" style="-fx-border-color: default; -fx-background-color: default;" text="Configurações" />
    <Label layoutX="124.0" layoutY="46.0" text="Vagas oferecidas">
      <font>
         <Font size="15.0" />
      </font></Label>
    <TableView fx:id="tableView" layoutY="80.0" prefHeight="488.0" prefWidth="360.0">
        <columns>
            <TableColumn fx:id="cId" prefWidth="37.0" text="Id" />
            <TableColumn fx:id="cVacancy" minWidth="0.0" prefWidth="84.0" text="Nome" />
            <TableColumn fx:id="cHierarch" minWidth="0.0" prefWidth="95.0" text="Hierarquia" />
            <TableColumn fx:id="cSalary" minWidth="0.0" prefWidth="71.0" text="Salário" />
         <TableColumn fx:id="cStatus" minWidth="0.0" prefWidth="72.0" text="Status" />
        </columns>
    </TableView>
   <Button fx:id="btnNewVacancy" layoutX="191.0" layoutY="580.0" mnemonicParsing="false" onAction="#addNewVacancy" prefHeight="46.0" prefWidth="149.0" text="Nova vaga">
      <font>
         <Font size="14.0" />
      </font>
   </Button>

</AnchorPane>
