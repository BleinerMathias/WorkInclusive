<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="640.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifsp.application.controller.candidacy.CandidacyPageUIController">
   <children>
      <Label layoutX="96.0" layoutY="46.0" prefHeight="21.0" prefWidth="169.0" text="Candidaturas pendentes">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
       <Button fx:id="btnhome" layoutX="18.0" layoutY="8.0" mnemonicParsing="false" onAction="#initialPage" style="-fx-border-color: default; -fx-background-color: default;" text="Inicio" />
       <Button fx:id="btnProfile" layoutX="75.0" layoutY="8.0" mnemonicParsing="false" onAction="#Account" style="-fx-border-color: default; -fx-background-color: default;" text="Perfil" />
       <Button fx:id="btnVacany" layoutX="124.0" layoutY="8.0" mnemonicParsing="false" onAction="#vacancy" style="-fx-border-color: default; -fx-background-color: default;" text="Vagas" />
       <Button fx:id="btnConfig" layoutX="253.0" layoutY="8.0" mnemonicParsing="false" onAction="#config" style="-fx-border-color: default; -fx-background-color: default;" text="Configurações" />
       <Button fx:id="btnInterviews" layoutX="180.0" layoutY="8.0" mnemonicParsing="false" onAction="#interviews" style="-fx-border-color: default; -fx-background-color: default;" text="Entrevistas" />
       <Button fx:id="btnDecline" layoutX="40.0" layoutY="548.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" text="Recusar" />
      <TableView fx:id="tableView" layoutY="83.0" prefHeight="428.0" prefWidth="360.0">
        <columns>
          <TableColumn fx:id="cId" prefWidth="45.0" text="Id" />
          <TableColumn fx:id="cVacancy" prefWidth="105.0" text="Vaga" />
            <TableColumn fx:id="cCandidate" prefWidth="122.0" text="Candidato" />
            <TableColumn fx:id="cStatus" prefWidth="87.0" text="Status" />
        </columns>
      </TableView>
      <Button fx:id="btnAccept" layoutX="220.0" layoutY="548.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" text="Aceitar" />
   </children>
</AnchorPane>
